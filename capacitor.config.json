      "splashFullScreen": true,
      "splashImmersive": true
    },
    "Preferences": {}
  },
  "android": {
    "minSdkVersion": 22,
    "compileSdkVersion": 34,
    "targetSdkVersion": 34
  },
  "cordova": {}
}
